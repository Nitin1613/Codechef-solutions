#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x;
	    cin>>x;
	    if ( x < 3 )
	    std::cout << "light" << std::endl;
	    else if(x >= 3 && x<7)
	    cout<<"moderate"<<endl;
	    else 
	    std::cout << "heavy" << std::endl;
	}
	return 0;
}
