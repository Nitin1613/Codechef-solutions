#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int x,y,z;
	    cin>>x>>y>>z;
	    if ( x > y && x>z)
	    cout<<"Alice"<<endl;
	    else if( y > z && y> x )
	    cout<<"bob"<<endl;
	    else
	    cout<<"charlie"<<endl;
	}
	return 0;
}
